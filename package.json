{
    "name": "@asafarim/booking-calendar",
    "version": "0.1.0",
    "description": "Google Calendar-style booking UI for FreelanceToolkit.Api",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
